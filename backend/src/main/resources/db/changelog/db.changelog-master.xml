<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">


<!--    Using for postgresql-->
    <preConditions>
        <dbms type="postgresql"/>
    </preConditions>

<!--    Tag-->
    <changeSet id="0" author="andrew">
        <tagDatabase tag="db_init"/>
    </changeSet>

    <include file="changes/v-1.0/db.changelog-v.1.0.xml" relativeToChangelogFile="true"/>

    <changeSet id="3" author="andrew">
        <tagDatabase tag="v.1.0"/>
    </changeSet>

    <include file="changes/v-2.0/db.changelog-v.2.0.xml" relativeToChangelogFile="true"/>

    <changeSet id="5" author="andrew">
        <tagDatabase tag="v.2.0"/>
    </changeSet>

    <include file="changes/v-3.0/db.changelog-v.3.0.xml" relativeToChangelogFile="true"/>

    <changeSet id="7" author="andrew">
        <tagDatabase tag="v.3.0"/>
    </changeSet>

    <include file="changes/v-4.0/db.changelog-v.4.0.xml" relativeToChangelogFile="true"/>

    <changeSet id="12" author="andrew">
        <tagDatabase tag="v.4.0-all"/>
    </changeSet>

    <include file="changes/v-5.0/db.changelog-v.5.0.xml" relativeToChangelogFile="true"/>

    <changeSet id="14" author="andrew">
        <tagDatabase tag="v.5.0-all"/>
    </changeSet>

<!--    New style of id -->
    <include file="changes/v-6.0/db.changelog-v.6.0.xml" relativeToChangelogFile="true"/>

    <changeSet id="60" author="andrew">
        <tagDatabase tag="v.6.0-all"/>
    </changeSet>



</databaseChangeLog>
